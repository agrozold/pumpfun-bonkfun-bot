# Solana RPC Endpoints (get your own keys!)
SOLANA_NODE_RPC_ENDPOINT=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY
SOLANA_NODE_WSS_ENDPOINT=wss://your-wss-endpoint

# Private Key (base58 encoded)
SOLANA_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# QuickNode
QUICKNODE_RPC_ENDPOINT=https://your-quicknode-endpoint
QUICKNODE_WSS_ENDPOINT=wss://your-quicknode-endpoint

# Optional: Other RPC providers
CHAINSTACK_RPC_ENDPOINT=https://your-chainstack-endpoint
ALCHEMY_RPC_ENDPOINT=https://your-alchemy-endpoint
DRPC_RPC_ENDPOINT=https://your-drpc-endpoint

# JITO Settings
JITO_ENABLED=true
JITO_TIP_LAMPORTS=200000
JITO_BLOCK_ENGINE_URL=https://frankfurt.mainnet.block-engine.jito.wtf
